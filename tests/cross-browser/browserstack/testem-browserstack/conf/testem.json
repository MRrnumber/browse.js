{
    "browser_start_timeout" : 300,
    "framework": "jasmine",
    "parallel": 4,

    "on_start": {
        "command": "kill -9 $(ps -A | grep BrowserStackLocal | grep -v grep | cut -d ' ' -f2); ./tests/cross-browser/browserstack/testem-browserstack/scripts/browserstack-local.js &",
        "wait_for_text": "Tunnel Started",
        "wait_for_text_timeout": 300000
    },

    "on_exit": "./tests/cross-browser/browserstack/testem-browserstack/scripts/browserstack-local.js `cat browserstack-local.pid`; rm browserstack-local.pid",

    "launchers": {
        "bs_chrome": {
            "exe": "./tests/cross-browser/browserstack/testem-browserstack/scripts/browserstack.js",
            "args": ["OSX", "ElCapitan", "chrome", "51.0", ""],
            "protocol": "browser"
        },
        "bs_firefox": {
            "exe": "./tests/cross-browser/browserstack/testem-browserstack/scripts/browserstack.js",
            "args": ["Windows", "10", "firefox", "45.0", ""],
            "protocol": "browser"
        },
        "bs_safari": {
            "exe": "./tests/cross-browser/browserstack/testem-browserstack/scripts/browserstack.js",
            "args": ["OSX", "ElCapitan", "safari", "9.1", ""],
            "protocol": "browser"
        },
        "bs_ie": {
            "exe": "./tests/cross-browser/browserstack/testem-browserstack/scripts/browserstack.js",
            "args": ["Windows", "10", "ie", "11.0", ""],
            "protocol": "browser"
        }
    },

    "launch_in_dev": ["Chrome","PhantomJS"],

    "launch_in_ci": [
        "bs_chrome",
        "bs_firefox",
        "bs_safari",
        "bs_ie"
    ],

    "src_files": [
        "browse.js",
        "tests/src/js/jasmine/*.js"
    ]
}
