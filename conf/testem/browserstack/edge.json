{
  "parallel": 2,
  "framework": "jasmine",
  "browser_start_timeout": 300,
  "src_files": [
    "lib/browse.js",
    "tests/js/jasmine/*.js"
  ],
  "on_start": {
    "command": "node ./node_modules/.bin/cbtr-testem-browserstack-open --local",
    "wait_for_text": "opened testem/browserstack",
    "wait_for_text_timeout": 60000
  },
  "on_exit": "node ./node_modules/.bin/cbtr-testem-browserstack-close",
  "launchers": {
    "Windows_10_Edge_14_0": {
      "exe": "node",
      "args": [
        "./node_modules/.bin/cbtr-testem-browserstack-browser",
        "--local",
        "--os",
        "Windows",
        "--osVersion",
        "10",
        "--browser",
        "edge",
        "--timeout",
        120,
        "--browserVersion",
        "14.0",
        "--screenshots",
        "--video"
      ],
      "protocol": "browser"
    },
    "Windows_10_Edge_15_0": {
      "exe": "node",
      "args": [
        "./node_modules/.bin/cbtr-testem-browserstack-browser",
        "--local",
        "--os",
        "Windows",
        "--osVersion",
        "10",
        "--browser",
        "edge",
        "--timeout",
        120,
        "--browserVersion",
        "15.0",
        "--screenshots",
        "--video"
      ],
      "protocol": "browser"
    }
  },
  "launch_in_ci": [
    "Windows_10_Edge_14_0",
    "Windows_10_Edge_15_0"
  ]
}
