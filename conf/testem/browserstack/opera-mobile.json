{
  "parallel": 2,
  "framework": "jasmine",
  "browser_start_timeout": 300,
  "src_files": [
    "lib/browse.js",
    "tests/js/jasmine/*.js"
  ],
  "on_start": {
    "command": "node ./node_modules/.bin/cbtr-testem-browserstack-open --local",
    "wait_for_text": "opened testem/browserstack",
    "wait_for_text_timeout": 60000
  },
  "on_exit": "node ./node_modules/.bin/cbtr-testem-browserstack-close",
  "launchers": {
    "Opera_OS_1024x600_Opera_Mobile_Browser_Samsung_Galaxy_Tab": {
      "exe": "node",
      "args": [
        "./node_modules/.bin/cbtr-testem-browserstack-browser",
        "--local",
        "--os",
        "opera",
        "--osVersion",
        "1024x600",
        "--browser",
        "Opera Browser",
        "--timeout",
        120,
        "--screenshots",
        "--video"
      ],
      "protocol": "browser"
    },
    "Opera_OS_1280x800_Opera_Mobile_Browser_Samsung_Galaxy_Tab_10_1": {
